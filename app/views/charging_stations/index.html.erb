<div class="layout-fixed-width ">
  <div class="hero">
  	<div class="hero-inner">
      <a href="" class="hero-logo"><img src="https://raw.githubusercontent.com/thoughtbot/refills/master/source/images/placeholder_logo_1.png
  " alt="Logo Image"></a>
  		<div class="hero-copy">
  			<h1>Charging Facilities Near Your Search</h1>
          <% if !@charging_stations.present? %>
            <h4>DERP. We didn't find any.</h4>
          <% end %>
          <% @charging_stations.each do |station| %>
            <div classs="listings">
              <div class="facility_name">
                <p>
                  <%= station.facility_name %>
                </p>
              </div>
              <div class="address">
                <ul>
                  <%= station.full_address %>
                </ul>
              </div>
            </div>
          <% end %>
  		</div>
  	</div>
  </div>
</div>
